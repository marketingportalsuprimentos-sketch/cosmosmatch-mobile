{
  "expo": {
    "name": "CosmosMatch",
    "slug": "cosmosmatch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#111827"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cosmosmatch.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#111827"
      },
      "package": "com.cosmosmatch.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "androidNavigationBar": {
      "visible": "immersive",
      "backgroundColor": "#00000000"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-video",
      [
        "expo-navigation-bar",
        {
          "position": "absolute",
          "visibility": "hidden",
          "behavior": "overlay-swipe",
          "backgroundColor": "#00000000"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "38e7d90e-1500-43a1-97c7-55ed467a6905"
      },
      "expo-navigation-bar": {
        "position": "absolute",
        "visibility": "hidden",
        "behavior": "overlay-swipe",
        "backgroundColor": "#00000000"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/38e7d90e-1500-43a1-97c7-55ed467a6905"
    }
  }
}
